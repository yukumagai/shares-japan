                      .clear
                .modal-footer
                  %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"} 中止
                  %button.btn.btn-primary{:type => "button"} 作成
          .menu-4{"data-target" => "#myModal4", "data-toggle" => "modal"}
